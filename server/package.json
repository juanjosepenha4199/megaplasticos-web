{
  "name": "megaplasticos-server",
  "version": "1.0.0",
  "description": "Backend server for Megaplasticos La Pradera e-commerce website",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
